{
    "name": "devpac/dashbaord",
    "description": "Easy and effective dashboard ",
    "keywords": [
        "devpac",
        "dashbaord"
    ],
    "homepage": "https://github.com/devpac/dashbaord",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Arun Selvan",
            "email": "arunbalajiselvan@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Devpac\\Dashbaord\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Devpac\\Dashbaord\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devpac\\Dashbaord\\DashbaordServiceProvider"
            ],
            "aliases": {
                "Dashbaord": "Devpac\\Dashbaord\\DashbaordFacade"
            }
        }
    }
}